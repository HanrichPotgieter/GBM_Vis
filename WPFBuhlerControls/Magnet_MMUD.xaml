<UserControl x:Class="WPFBuhlerControls.Magnet_MMUD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="20">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="20" Height="50">
                <Polyline Name="polyTop" Points="0.3,10.5 5,0 15,0 19.5,10 0.5,10" StrokeThickness="0.7" Stroke="Black"/>
                <Rectangle Name="Rectmain" Width="20" Height="30" Margin="0,10" StrokeThickness="0.7" Stroke="Black"/>
                <Polyline Name="polyBot" Points="0.3,39.5 5,50 15,50 19.5,40 0.5,40" StrokeThickness="0.7" Stroke="Black"/>

                <Polyline Name="polyMidLeft" Points="4,19 8,19 8,29 4,29" StrokeThickness="1.5" Stroke="Black"/>
                <Polyline Name="polyMidRight" Points="16,19 12,19 12,29 16,29" StrokeThickness="1.5" Stroke="Black"/>
            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
