                <Rectangle Name="rectBottom" Width="18" Height="22" Margin="11,23" Stroke="Black" StrokeThickness="1"/>
                <Polyline Name="polyMain" Stroke="Black" StrokeThickness="0.7" Points="11.1,44.5 15,48 25,48 28.9,44.5" />
                <Ellipse Name="ellipseMain" Width="13" Height="13" Stroke="Black" StrokeThickness="0.7" Fill="White" Margin="34,12"/>
                <Ellipse Width="8" Height="8" Stroke="Black" StrokeThickness="0.7" Margin="36.6,14.5"/>

            </Canvas>
        </Viewbox>
