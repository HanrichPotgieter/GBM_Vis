             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="35" d:DesignWidth="37">
    <Grid >
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="37" Height="35" >
                <Rectangle Stroke="Black" Width="37" Height="35" Fill="Azure" />
                <Polyline Stroke="Black" Name="polyAlarmHorn" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="5,12 10,12 20,2 20,32 10,22 5,22 5,12" />
                <Polyline Stroke="Black" Name="polyLine1" StrokeThickness="1" StrokeLineJoin="Round"  Points="23,12 33,7" />
                <Polyline Stroke="Black" Name="polyLine2" StrokeThickness="1" StrokeLineJoin="Round"  Points="23,17 33,17" />
                <Polyline Stroke="Black" Name="polyLine3" StrokeThickness="1" StrokeLineJoin="Round"  Points="23,22 33,27" />
            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
