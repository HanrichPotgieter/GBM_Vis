<UserControl x:Class="WPFBuhlerControls.Feeder_MicroDifferential_1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="130" d:DesignWidth="90">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="90" Height="130">

                <Polyline Name="polyHopper" Stroke="Black" StrokeThickness="1" Points="15,60 16,0 60,0 50,60 15,60"/>
                
                <Rectangle Name="RectMain" Margin="20,110" Width="70" Height="20" StrokeThickness="1" Stroke="Black"/>
                <Polyline Name="polyBin" Margin="0,60" Stroke="Black" StrokeThickness="1" Points="20.4,50.6 15,0 50,0 40,50.5 20,50.5"/>
                <Polyline Stroke="Black" StrokeThickness="1" Margin="0,60" Points="25,65 35,55 45,65 55,55 65,65 75,55 85,65" />

                <Polyline Name="polyLine1" Stroke="Black" Margin="0,60" StrokeThickness="1.5" Points="22,25 38,25" />
                <Polyline Name="polyLine2" Stroke="Black" Margin="0,60" StrokeThickness="1.5" Points="31,25 31,15 24,15" />


            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
