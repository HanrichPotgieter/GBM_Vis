<UserControl x:Class="WPFBuhlerControls.Label_T1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="20">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="20" Height="20">
                <Ellipse Name="elipseMain" Width="20" Height="20" StrokeThickness="1" Stroke="Black"/>
                <Label Canvas.Left="-1" Canvas.Top="-4" Content="T1" Height="28" Name="label1" />
            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
