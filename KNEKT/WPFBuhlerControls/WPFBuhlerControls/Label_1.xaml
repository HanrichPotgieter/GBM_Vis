<UserControl x:Class="WPFBuhlerControls.Label_1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="20">
    <Viewbox Stretch="Uniform">
        <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="20" Height="20">
            <Ellipse Name="elipseMain" Width="20" Height="20" StrokeThickness="1" Stroke="Black"/>
            <Polyline Name="polyB" Points=" 6,15 14,15" StrokeThickness="0.7" Stroke="Black"/>
            <Polyline Name="polyT" Points=" 10,15 10,4 6,6" StrokeThickness="0.7" Stroke="Black"/>
        </Canvas>
    </Viewbox>
</UserControl>
