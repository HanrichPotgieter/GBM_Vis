<UserControl x:Class="WPFBuhlerControls.Sifter_MPAR"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="75" d:DesignWidth="60">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="60" Height="75">

                <Rectangle Name="RectMain1" Width="60" Height="15.5" Margin="0,0" StrokeThickness="1" Stroke="Black"/>
                <Rectangle Name="RectMain2" Width="60" Height="15.5" Margin="0,15" StrokeThickness="1" Stroke="Black"/>
                <Rectangle Name="RectMain3" Width="60" Height="15.5" Margin="0,30" StrokeThickness="1" Stroke="Black"/>
                <Rectangle Name="RectMain4" Width="60" Height="15.5" Margin="0,45" StrokeThickness="1" Stroke="Black"/>
                <Rectangle Name="RectMain5" Width="60" Height="15.5" Margin="0,60" StrokeThickness="1" Stroke="Black"/>
            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
