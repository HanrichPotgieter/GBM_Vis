<UserControl x:Class="WPFBuhlerControls.Bin_Discharger_MFPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            d:DesignHeight="35" d:DesignWidth="60">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="60" Height="35">
                
                <Polyline Name="polyMain" Points="0,0 0,10 30,25 60,10 60,0 0,0 0,15" StrokeThickness="1" Stroke="Black"/>
                <Polyline Name="polySmall" Points="0,10 0,20 30,35 60,20 60,10 30,25 0,10" StrokeThickness="1" Stroke="Black"/>
                

            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>

