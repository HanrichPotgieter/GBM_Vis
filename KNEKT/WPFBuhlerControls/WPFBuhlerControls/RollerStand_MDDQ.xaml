<UserControl x:Class="WPFBuhlerControls.RollerStand_MDDQ"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="140" d:DesignWidth="110">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" Name="canvasTop" Width="110" Height="140">


                <Polyline Name="PolyLeftMid" Points="45,10 55,10 55,140 15,140 15,50 40,35 40,35 45,10" Stroke="Black" StrokeThickness="1"/>
                <Polyline Name="PolyRightMid" Points="65,10 55,10 55,140 95,140 95,50 70,35 65,10" Stroke="Black" StrokeThickness="1"/>
                
                <Polyline Name="rectTopLeft" Points="0,95 15,95 15,50, 0,50 0,95" Stroke="Black" StrokeThickness="1"/>
                <Polyline Name="rectTopRight" Points="95,95 110,95 110,50, 95,50 95,95" Stroke="Black" StrokeThickness="1"/>
                
                <Polyline Name="rectBotLeft" Points="0,140 15,140 15,95, 0,95 0,140" Stroke="Black" StrokeThickness="1"/>
                <Polyline Name="rectBotRight" Points="95,140 110,140 110,95, 95,95 95,140" Stroke="Black" StrokeThickness="1"/>
            </Canvas>
        </Viewbox>


    </Grid>
</UserControl>
