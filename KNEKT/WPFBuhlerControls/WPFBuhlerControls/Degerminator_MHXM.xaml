<UserControl x:Class="WPFBuhlerControls.Degerminator_MHXM"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            d:DesignHeight="60" d:DesignWidth="80">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Height="60"  Width="80">

                <Rectangle Name="RectMain" Width="50" Height="17" Margin="30,23" StrokeThickness="1" Stroke="Black"/>
                <Polyline Name="polyBot" Stroke="Black" StrokeThickness="1" Points="79.5,39.5 75,60 65,60 60,39.5 80,39.5"/>
                <Polyline Name="polyConnect" Stroke="Black" StrokeThickness="1" Points="30,23.5 26.5,23.5 25.1,27 23,29.5 22.5,30 21.2,31 18.7,31.9 17,32.4 30.5,32.4 30.5,23.5"/>
                <Ellipse Name="ellipseBig" Width="23" Height="23" Stroke="Black" Margin="4,10"/>
                <Ellipse Name="ellipseSmall" Width="10" Height="10" Stroke="Black" Margin="10.5,16.5" />


            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
