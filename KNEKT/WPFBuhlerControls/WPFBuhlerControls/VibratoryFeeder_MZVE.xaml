             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            d:DesignHeight="30" d:DesignWidth="40">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" Name="canvasmain" Width="40" Height="30">

                <Rectangle Name="rectSmallTop" Width="10" Height="5" Margin="0,0" Stroke="Black" StrokeThickness="1"/>
                <Rectangle Name="rectMid" Width="40" Height="10" Margin="0,4" Stroke="Black" StrokeThickness="1"/>
                <Rectangle Name="rectSmallBot" Width="10" Height="5" Margin="30,13" Stroke="Black" StrokeThickness="1"/>
                
                <Polyline Name="polyBottom1"  Points="0.7,13.5 5,23 25,23 20,13.5 0.7,13.5 5,23" Stroke="Black" StrokeThickness="1"/>
                <Polyline Name="polyBottom2"  Points="5,23 6.5,26 26.7,26 25,23 5,23 6.5,26 " Stroke="Black" StrokeThickness="1"/>

            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
