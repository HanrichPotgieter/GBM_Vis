<UserControl x:Class="WPFBuhlerControls.Destoner_MTSD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
              d:DesignHeight="60" d:DesignWidth="50">
    <Grid>
        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Width="50" Height="60">

                <Polyline Name="PolyMain" Stroke="Black" StrokeThickness="1" Points="0,10 30,10 45,25 44,45 25,45 25,40 0,35 0,10 30,10"/>

                <Polyline Stroke="Black" StrokeDashArray="5" StrokeThickness="0.7" Points="0,23 45,32"/>
                <Polyline Stroke="Black" StrokeDashArray="5" StrokeThickness="0.7" Points="0,29 38,37"/>
                <Polyline Stroke="Black" Points="38,37 38,45" StrokeThickness="0.7"/>

                <Rectangle Name="RectMain" Height="8" Width="5" Stroke="Black" Margin="2.5,2.4"/>

                <Polyline Name="PolyFrame" Stroke="Black" Points="0,0 10,0 10,50 30,50 30,45 33,45 33,58 30,58 30,53 10,53 10,58 7,58 7,3 0,3 0,0 10,0" Fill="White"/>



            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
