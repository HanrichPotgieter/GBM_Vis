<UserControl x:Class="WPFBuhlerControls.Pellet_Dispenser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="17"
             >
    <Grid>

        <Viewbox Stretch="Uniform">
            <Canvas HorizontalAlignment="Center" VerticalAlignment="Center" Name="canvasMain" Height="30" Width="17">
                <Rectangle Name="rectMain" Height="15" Width="10" Stroke="Black" StrokeThickness="1" Margin="5,0" />
                <Polyline Name="PolyMain" Stroke="Black" StrokeThickness="1" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Points="15,15.3 5,15.3 0,30 17,30" />
                <Polyline Name="PolysLine" Stroke="Black" StrokeThickness="1" Points="10,15 10,30" />
            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
