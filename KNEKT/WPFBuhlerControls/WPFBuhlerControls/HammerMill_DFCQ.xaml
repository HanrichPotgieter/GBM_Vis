                <Ellipse Name="ellipseMain" Width="19" Height="19" Margin="15.5,20" Stroke="Black" StrokeThickness="0.7"/>                

                <Polyline Stroke="Black" Name="polyMain" StrokeThickness="0.7" Points="15,45  13,40  10,35 10,20  15,16  20,10 30,10  35,16  40,20 40,35 37,40 35,45 15,45"/>
                <Polyline Stroke="Black" Name="polyBottom" StrokeThickness="0.7" Points="15.1,45 34.9,45 25,60 15.1,45 30,45"/>

                <Polyline Stroke="Black" Name="polyLeftLine" StrokeThickness="1" Points="12,43 12,15"/>
                <Polyline Stroke="Black" Name="polyRightLine" StrokeThickness="1" Points="38,43 38,15"/>




            </Canvas>
        </Viewbox>
    </Grid>
</UserControl>
