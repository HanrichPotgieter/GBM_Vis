<Window x:Class="KNEKT.DisplayPages.HelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="HelpWindow" Width="1280" Height="800"
        MinWidth="1024" MinHeight="768" WindowStartupLocation="CenterScreen" WindowStyle="None" WindowState="Maximized" xmlns:WPFBuhlerControls="clr-namespace:WPFBuhlerControls;assembly=WPFBuhlerControls" >

    <Window.Resources>
        <ControlTemplate TargetType="Button" x:Key="NavigationButtonTemplateLeft">
            <StackPanel Orientation="Horizontal" Width="180" Height="80" Background="Black" >
                <Grid Name="gridLeftArrow" Width="80" Height="80" >
                    <Ellipse Stroke="White" StrokeThickness="8" Width="80" Height="80" x:Name="Ellipse1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,40 60,40" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,40 35,25" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly2"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,40 35,55" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly3"/>
                </Grid>
                <Label Foreground="White" FontSize="40" Content="Back" Height="80" VerticalContentAlignment="Center" FontFamily="Comic Sans MS" x:Name="Label1"/>
            </StackPanel>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="Ellipse1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly2" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly3" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Label1" Property="Foreground" Value="Red"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
        <ControlTemplate TargetType="Button" x:Key="NavigationButtonTemplateRight" >
            <StackPanel Orientation="Horizontal" Width="200" Height="80" Background="Black">
                <Label Foreground="White" FontSize="40" Content="Next" Height="80" VerticalContentAlignment="Center" FontFamily="Comic Sans MS" x:Name="Label1"/>
                <Grid Name="gridLeftArrow" Width="80" Height="80">
                    <Ellipse Stroke="White" StrokeThickness="8" Width="80" Height="80" x:Name="Ellipse1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,40 60,40" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="60,40 45,25" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly2"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="60,40 45,55" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly3"/>
                </Grid>
            </StackPanel>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="Ellipse1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly2" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly3" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Label1" Property="Foreground" Value="Red"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

        <ControlTemplate TargetType="Button" x:Key="NavigationButtonTemplateExit" >
            <StackPanel Orientation="Horizontal" Width="60" Height="60" Background="Black">
                <Grid Name="gridLeftArrow" Width="60" Height="60">
                    <Ellipse Stroke="White" StrokeThickness="8" Width="60" Height="60" x:Name="Ellipse1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,20 40,40" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly1"/>
                    <Polyline Stroke="White" StrokeThickness="5" Points="20,40 40,20" StrokeEndLineCap="Round" StrokeStartLineCap="Round" x:Name="Poly2"/>
                </Grid>
            </StackPanel>
            <ControlTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="true">
                    <Setter TargetName="Ellipse1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly1" Property="Stroke" Value="Red"/>
                    <Setter TargetName="Poly2" Property="Stroke" Value="Red"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>


        <Style TargetType="TextBlock">
            <Setter Property="FontFamily" Value="Comic Sans MS"/>
            <Setter Property="FontSize" Value="30"/>
            <Setter Property="Padding" Value="2" />
            <Setter Property="Visibility" Value="Hidden" />
        </Style>

        <Style TargetType="Path">
            <Setter Property="StrokeThickness" Value="6" />
        </Style>

    </Window.Resources>

    <Grid Background="Black" Opacity="0.8" Name="GridMain">

        <Button Content="Exit Help" Height="85" Margin="595,0,584,3" Name="btnExitHelp" Click="btnExitHelp_Click" VerticalAlignment="Bottom"  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Template="{StaticResource NavigationButtonTemplateExit}"/>
        <Button HorizontalAlignment="Left" Margin="6,568,0,112" Name="btnBack" Width="256" Click="btnBack_Click" Template="{StaticResource NavigationButtonTemplateLeft}" />
        <Button Content="NEXT" Margin="0,568,24,112" Name="btnNext" Click="btnNext_Click" HorizontalAlignment="Right" Width="188" Template="{StaticResource NavigationButtonTemplateRight}"/>


        <!--Start Help-->
        <Grid x:Name="gridStartHelp" >
            <TextBlock Margin="417,390,441,271" Name="gridStartHelpGridText" Text="Press Next to begin" TextWrapping="Wrap" Foreground="White"  TextAlignment="Center" TextBlock.FontWeight="Bold" TextBlock.FontSize="40" />
        </Grid>
        <!--Start Help-->

        <!--Alarm Viewer-->
        <Grid x:Name="gridAlarmViewer" Opacity="0.4">
            <Image Height="27" HorizontalAlignment="Left" Margin="1,25,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="24" Source="/KNEKT;component/Images/Bell.png" StretchDirection="Both"  />
            <TextBlock Height="100" Width="400" HorizontalAlignment="Left" Margin="59,181,0,0" Name="gridAlarmViewerGridText" Text="Clicking this icon will open the Alarm/Event viewer" VerticalAlignment="Top" TextWrapping="Wrap" Foreground="White"   />
            <Grid x:Name="textLineAlarmViewer" Opacity="1"  Margin="16,35,0,0" HorizontalAlignment="Left" Width="250" Visibility="Hidden" Height="150" VerticalAlignment="Top">
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 0,20 C 140,150 180,90 220,150" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Alarm Viewer-->

        <!--Alarm Messages-->
        <Grid x:Name="gridAlarmMessages" Opacity="0.4">
            <TextBox BorderBrush="Gray" Height="27" Margin="23,25,318,0" Name="txtAlarms" VerticalAlignment="Top" FontSize="10"  Focusable="False" IsReadOnly="True">
                <TextBox.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,10">
                        <GradientStop Offset="0.0" Color="White"/>
                        <GradientStop Offset="0.1" Color="lime"/>
                    </LinearGradientBrush>
                </TextBox.Background>
                <TextBox.Text>No Active Alarms</TextBox.Text>
            </TextBox>
            <TextBlock Height="140" Width="400" HorizontalAlignment="Left" Margin="205,274,0,0" Name="gridAlarmMessagesGridText" Text="This will display the latest alarm that has occured if there are any" VerticalAlignment="Top" TextWrapping="Wrap" Foreground="White"   />
            <Grid x:Name="textLineAlarmMessages" Opacity="1"  Margin="59,43,0,0" HorizontalAlignment="Left" Width="320" Visibility="Hidden" Height="240" VerticalAlignment="Top">
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Path Stroke="White" StrokeThickness="7" Data="M 150,10 C 150,80 260,110 300,240" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>
                </Canvas>
            </Grid>
        </Grid>
        <!--Alarm Messages-->



        <!--PAGES-->
        <Grid x:Name="gridLineButtons" Opacity="0.4">

            <Button Content="PAGE1" Height="35" Margin="6,0,0,8" Name="btnLine1" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75"  Focusable="False" />
            <Button Content="PAGE2" Height="35" Margin="87,0,0,8" Name="btnLine2" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="75"  Focusable="False"/>
            <TextBlock Width="400" HorizontalAlignment="Left" Margin="261,0,0,176" Name="gridLineButtonsGridText" Text="You can use these buttons to navigate to different lines in the plant" TextWrapping="Wrap" Foreground="White" Height="160" VerticalAlignment="Bottom" />
            <Grid x:Name="textLineLineButtons" Opacity="1" Margin="64,0,0,20" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 0,270 C 100,50 350,330 350,125" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>

        </Grid>
        <!--PAGES-->

        <!--CONTROL BUTTONS-->

        <!--Start Button-->
        <Grid x:Name="gridStartButton" Opacity="0.4">
            <Button Height="45" HorizontalAlignment="Right" Margin="0,0,271,4" Name="btnControlStart" VerticalAlignment="Bottom" Width="47" ToolTip="Start Line"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="-8,0, -8,-15 10,0 -8,15 -8,0"/>
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Margin="0,0,347,198" Name="gridStartButtonGridText" Text="This button starts the active process line" TextWrapping="Wrap" Foreground="White" Height="90" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="340"  />
            <Grid x:Name="textLineStartButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,298,30" Height="300" VerticalAlignment="Bottom" Visibility="Hidden" HorizontalAlignment="Right" Width="380">
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 355,260 C 120,80 120,310 100,135" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Start Button-->

        <!--Pause Button-->
        <Grid x:Name="gridPauseButton" Opacity="0.4">
            <Button  Height="45" HorizontalAlignment="Right" Margin="0,0,218,4" Name="btnControlSuspend" VerticalAlignment="Bottom" Width="47" ToolTip="Pause Feed"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="-10,0, -10,-15 -2,-15 -2,15 -10,15 -10,0" />
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="10,0, 10,-15 2,-15 2,15 10,15 10,0" />
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Height="90" Margin="0,0,242,199" Name="gridPauseButtonGridText" Text="This button stops the feed of the process line" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White" HorizontalAlignment="Right" Width="410" />
            <Grid x:Name="textLinePauseButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,233,31" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 355,270 C 220,80 120,310 100,135" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Pause Button-->

        <!--Stop Button-->
        <Grid x:Name="gridStopButton" Opacity="0.4">
            <Button Height="45" HorizontalAlignment="Right" Margin="0,0,165,4" Name="btnControlSeqStop" VerticalAlignment="Bottom" Width="47" ToolTip="Sequence Stop Line"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="-12,2, -12,-12 12,-12 12,12 -12,12 -12,2" />
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Height="130" Width="410" HorizontalAlignment="Right" Margin="0,0,252,176" Name="gridStopButtonGridText" Text="This button performs a sequence stop on the process line" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"  />
            <Grid x:Name="textLineStopButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,184,41" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 355,270 C 220,80 120,310 100,135" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Stop Button-->

        <!--Acknowledge Button-->
        <Grid x:Name="gridAcknowledgeButton" Opacity="0.4">
            <Button Height="45" HorizontalAlignment="Right" Margin="0,0,112,4" Name="btnControlAcknowledge" VerticalAlignment="Bottom" Width="47" ToolTip="Fault Acknowledge"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="Lime" Points="-15,-3 -5,5 15,-15 -5,15 -15,-3" />
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Height="100" Width="450" HorizontalAlignment="Right" Margin="0,0,188,199" Name="gridAcknowledgeButtonGridText" Text="This button will acknowledge the faults on the process line" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"  />
            <Grid x:Name="textLineAcknowledgeButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,132,41" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Path Stroke="White" StrokeThickness="7" Data="M 355,270 C 220,80 120,310 100,135" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Acknowledge Button-->

        <!--Mute Siren Button-->
        <Grid x:Name="gridMuteSirenButton" Opacity="0.4">
            <Button Height="45" HorizontalAlignment="Right" Margin="0,0,59,4" Name="btnControlMuteSiren" VerticalAlignment="Bottom" Width="47" ToolTip="Mute Siren"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="0.5" StrokeLineJoin="Round" Fill="DarkGray" Points="-15,-5 -10,-5 5,-15 5,15 -10,5 -15,5 -15,-5" />
                        <Polyline Stroke="Black" StrokeThickness="1.5" StrokeLineJoin="Round" Fill="DarkGray" Points="8,-5 18,5 " />
                        <Polyline Stroke="Black" StrokeThickness="1.5" StrokeLineJoin="Round" Fill="DarkGray" Points="18,-5 8,5 " />
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Height="100" Width="450" HorizontalAlignment="Right" Margin="0,0,234,251" Name="gridMuteSirenButtonGridText" Text="This button will silence the alarm horn when there's a fault" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"  />
            <Grid x:Name="textLineMuteSirenButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,75,53" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 355,295 C 220,140 120,350 100,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Mute Siren Button-->


        <!--Emergencey Stop Button-->
        <Grid x:Name="gridEmergencyStopButton" Opacity="0.4">
            <Button Height="45" Margin="0,0,6,4" Name="btnControlEmergencyStop" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="47" ToolTip="Emergency Stop"  Focusable="False">
                <Button.Content>
                    <Canvas>
                        <Polyline Stroke="Black" StrokeThickness="1" StrokeLineJoin="Round" Fill="Red" Points="-15,8 -15,-8 -7.5,-15 7.5,-15 15,-8 15,8 7.5,15 -7.5,15 -15,8" />
                        <Polyline Stroke="White" StrokeThickness="1.5" StrokeLineJoin="Round" Points="-8,-6 -12,-6 -12,0, -8,0 -8,6 -12,6" />
                        <Polyline Stroke="White" StrokeThickness="1.5" StrokeLineJoin="Round" Points="-1,-6 -6,-6" />
                        <Polyline Stroke="White" StrokeThickness="1.5" StrokeLineJoin="Round" Points="-3.5,-6 -3.5,7" />
                        <Polyline Stroke="White" StrokeThickness="1.5" StrokeLineJoin="Round" Points="1,-6 5,-6 5,6 1,6 1,-6.7" />
                        <Polyline Stroke="White" StrokeThickness="1.5" StrokeLineJoin="Round" Points="8,6 8,-6 12,-6 12,0 8,0" />
                    </Canvas>
                </Button.Content>
            </Button>
            <TextBlock Height="100" Width="500" HorizontalAlignment="Right" Margin="0,0,186,245" Name="gridEmergencyStopButtonGridText" Text="This button will perform an emergency stop on the process line" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"  />
            <Grid x:Name="textLineEmergencyStopButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,20,48" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 355,295 C 220,140 120,350 100,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Emergencey Stop Button-->

        <!--Element Descriptions-->
        <Grid x:Name="gridElementDesc" Opacity="0.4">
            <TextBox BorderBrush="Gray" Padding="0,4" HorizontalAlignment="Left" Margin="1,0,0,53" Name="txtElDesc" Width="137"  Text="Element Description" Focusable="False" Background="LightGray" Height="35" VerticalAlignment="Bottom" FontSize="14" IsReadOnly="True"/>
            <TextBox Height="35" Margin="136,0,422,53" Padding="0,4" Name="txtElementDescription" VerticalAlignment="Bottom" BorderBrush="Gray" Focusable="False" FontSize="14" IsReadOnly="True"/>
            <TextBlock Height="135" Width="500" HorizontalAlignment="Left" Margin="295,0,0,234" Name="gridElementDescGridText" Text="The selected element's description will be shown here once the element is clicked or touched" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White" />
            <Grid x:Name="textLineElementDesc" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="177,0,0,68" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 0,270 C 100,50 350,330 350,125" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Element Descriptions-->

        <!--Element Status-->
        <Grid x:Name="gridElementStatus" Opacity="0.4">
            <TextBox Background="LightGray" Padding="0,4" BorderBrush="Gray" Focusable="False" Height="35" Margin="0,0,376,53" Name="txtElStat" Text="Status" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="48" FontSize="14" IsReadOnly="True"/>
            <TextBox Height="35" Padding="0,4" HorizontalAlignment="Right" Margin="0,0,203,53" Name="txtElementStatus" VerticalAlignment="Bottom" Width="174" BorderBrush="Gray" Focusable="False" FontSize="14" IsReadOnly="True"/>
            <TextBlock Height="100" Width="450" HorizontalAlignment="Right" Margin="0,0,308,243" Name="gridElementStatusGridText" Text="The status of the the selected element will be displayed here" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"   TextAlignment="Center" />
            <Grid x:Name="textLineElementStatus" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,198,61" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 230,265 C 200,130 100,320 110,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round" ></Path>

                </Canvas>
            </Grid>
        </Grid>
        <!--Element Status-->

        <!--PLC Comms-->
        <Grid x:Name="gridPLCComms" Opacity="0.4">
            <TextBox Background="LightGray" Padding="0,4" BorderBrush="Gray" Focusable="False" Height="35" Margin="0,0,118,53" Name="txtPlcCom" Text="PLC Comms" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="86" FontSize="14" IsReadOnly="True"/>
            <TextBox BorderBrush="Gray" Padding="0,4" Focusable="False" FontSize="14" TextAlignment="Center" Margin="0,0,1,53" Name="txtPLCComm" Foreground="White" Height="35" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="118" IsReadOnly="True"/>
            <TextBlock Height="165" Width="500" HorizontalAlignment="Right" Margin="0,0,220,221" Name="gridPLCCommsGridText" Text="The status of communication that the SCADA has with the PLC is displayed here" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"   TextAlignment="Center"  />
            <Grid x:Name="textLinePLCComms" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,162,61" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

                    <Path Stroke="White" StrokeThickness="7" Data="M 370,265 C 240,130 100,320 110,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>

                </Canvas>
            </Grid>

        </Grid>
        <!--PLC Comms-->

        <!--Help Button-->
        <Grid x:Name="gridHelpButton" Opacity="0.4">
            <Button Height="35" HorizontalAlignment="Right" HorizontalContentAlignment="Left" Margin="0,0,425,9" Name="btnHelp" VerticalAlignment="Bottom" Width="35"  Focusable="False">
                <DockPanel >
                    <Image DockPanel.Dock="Left" Source="..\..\Images\HelpIcon.png" />
                </DockPanel>
            </Button>
            <TextBlock Height="106" Margin="0,0,324,201" Name="gridHelpButtonGridText" Text="Press this button to open the help screen" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White" HorizontalAlignment="Right" Width="378" />
            <Grid x:Name="textLineHelpButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,296,17" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Path Stroke="White" StrokeThickness="7" Data="M 230,265 C 200,130 100,320 110,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>
                </Canvas>
            </Grid>
        </Grid>
        <!--Help Button-->

        <!--Reporting Button-->
        <Grid x:Name="gridReportingButton" Opacity="0.4">
            <Button Focusable="False" Height="35" HorizontalAlignment="Right" HorizontalContentAlignment="Center" Margin="0,0,376,9" Name="btnReports" VerticalAlignment="Bottom" Width="42" >
                <DockPanel>
                    <Image DockPanel.Dock="Left" Source="..\..\Images\Report-Icon.png" />
                </DockPanel>
            </Button>
            <TextBlock Height="140" Width="450" HorizontalAlignment="Right" Margin="0,0,255,156" Name="gridReportingButtonGridText" Text="If reporting is available, this button will navigate to the reporting applicaiton" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"  />
            <Grid x:Name="textLineReportingButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,247,17" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Path Stroke="White" StrokeThickness="7" Data="M 230,265 C 200,30 100,300 90,155" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>
                </Canvas>
            </Grid>
        </Grid>
        <!--Reporting Button-->

        <!--Job Button-->
        <Grid x:Name="gridJobButton" Opacity="0.4">
            <Button Focusable="False" Height="45" Margin="0,0,324,4" Name="btnJob" VerticalAlignment="Bottom"  HorizontalAlignment="Right" Width="47" >
                <DockPanel Height="40" Width="40">
                    <Image Source="..\..\Images\JobSelection.png" Width="40" Height="40"/>
                </DockPanel>
            </Button>
            <TextBlock Height="150" Margin="0,0,268,215" Name="gridJobButtonGridText" Text="If there are senders and receivers available for the selected process line this button will display the job form" VerticalAlignment="Bottom" TextWrapping="Wrap" Foreground="White"   HorizontalAlignment="Right" Width="550" Visibility="Visible" />
            <Grid x:Name="textLineJobButton" Opacity="1" Canvas.Top="420" Canvas.Left="59" Margin="0,0,252,26" Height="300" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="380" Visibility="Hidden" >
                <Canvas HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                    <Path Stroke="White" StrokeThickness="7" Data="M 280,265 C 220,50 100,300 80,105" StrokeEndLineCap="Round" StrokeStartLineCap="Round"></Path>
                </Canvas>
            </Grid>
        </Grid>
        <!--Job Button-->

        <!-- Line Name TextBox -->
        <Grid x:Name="gridLineName" >
            <TextBox BorderBrush="Gray" Focusable="False" Height="25" Margin="1,1,151,0" Name="txtLineName" VerticalAlignment="Top" Background="LightGray" IsReadOnly="True"/>
