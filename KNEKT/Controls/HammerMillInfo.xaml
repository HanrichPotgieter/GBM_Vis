        <Label Content="Running Days (Rev): " FontSize="9" Height="17" HorizontalAlignment="Left" HorizontalContentAlignment="Left" Margin="7,123,0,0" Name="label6" Padding="0,0,0,0" VerticalAlignment="Top" Width="85" />
